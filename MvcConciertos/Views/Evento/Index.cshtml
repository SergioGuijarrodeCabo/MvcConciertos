@model IEnumerable<MvcConciertos.Models.Evento>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>


<table class="table">
    <thead>
        <tr>
            <th>IdEevento</th>
            <th>Nombre</th>
            <th>Artista</th>
            <th>IdCategoria</th>
            <th>Imagen</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.IdEevento</td>
                <td>@item.Nombre</td>
                <td>@item.Artista</td>
                <td>
                    <img src="@item.Imagen" width="150" />
                </td>
                <td>@item.IdCategoria</td>
         
            </tr>
        }
    </tbody>
</table>
